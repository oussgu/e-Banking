<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="e-bankingv1" parallel="classes" thread-count="0">
  
  <listeners>
    <listener class-name="com.ebanking.utilities.Reporting" />
  </listeners>

  <test name="BankingApp Test">
    <parameter name="browser" value="chrome" />
    <classes>
      <class name="com.ebanking.testCases.TC_LoginTest_001" />
      <class name="com.ebanking.testCases.TC_LoginDDT_002" />
      <class name="com.ebanking.testCases.TC_AddCustomerTest_003" />
    </classes>
  </test>
  
   <test name="BankingApp Test Mse">
    <parameter name="browser" value="Mse" />
    <classes>
      <class name="com.ebanking.testCases.TC_LoginTest_001" />
      <class name="com.ebanking.testCases.TC_LoginDDT_002" />
      <class name="com.ebanking.testCases.TC_AddCustomerTest_003" />
    </classes>
  </test>
  
   <test name="BankingApp Test firefox">
    <parameter name="browser" value="firefox" />
    <classes>
      <class name="com.ebanking.testCases.TC_LoginTest_001" />
      <class name="com.ebanking.testCases.TC_LoginDDT_002" />
      <class name="com.ebanking.testCases.TC_AddCustomerTest_003" />
    </classes>
  </test>

</suite>